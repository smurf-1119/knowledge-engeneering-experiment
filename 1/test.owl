###  http://www.seu.edu.cn/ontologies/pizza.owl#Flavor
:Flavor rdf:type owl:ObjectProperty ;
        rdfs:domain :Pizza ;
        rdfs:range :PizzaFlavor .


###  http://www.seu.edu.cn/ontologies/pizza.owl#hasBase
:hasBase rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :hasIngredient .


###  http://www.seu.edu.cn/ontologies/pizza.owl#hasIngredient
:hasIngredient rdf:type owl:ObjectProperty .


###  http://www.seu.edu.cn/ontologies/pizza.owl#hasTopping
:hasTopping rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasIngredient ;
            rdfs:domain :Pizza ;
            rdfs:range :PizzaTopping .


#################################################################
#    Data properties
#################################################################

###  http://www.seu.edu.cn/ontologies/pizza.owl#hasCalorificContentValue
:hasCalorificContentValue rdf:type owl:DatatypeProperty ;
                          rdfs:domain :Pizza ;
                          rdfs:range xsd:double .


#################################################################
#    Classes
#################################################################

###  http://www.seu.edu.cn/ontologies/pizza.owl#CheesePizza
:CheesePizza rdf:type owl:Class ;
